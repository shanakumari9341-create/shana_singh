#include<iostream>
using namespace std;

class A {
public:
    A() {
        cout << " Constructor Called" << endl;
    }
    ~A() {
        cout << " Destructor Called" << endl;
    }
};

class B : public A {
public:
    B() {
        cout << " Constructor Called" << endl;
    }
    ~B() {
        cout << " Destructor Called" << endl;
    }
};

int main() {
    B obj;   // Child Object
    return 0;
}
